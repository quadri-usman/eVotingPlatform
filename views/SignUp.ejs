<link
  href="https://fonts.googleapis.com/css2?family=Nunito Sans:wght@600&family=Josefin Sans:wght@600&family=Typold:wght@600&family=Oswald:wght@300;400&display=swap"
  rel="stylesheet"
/>
<link rel="stylesheet" href="/SignUp.css" />

<form action="/register" method="post" class="signupform">
  <div class="signupeVote">
    <div id="logo">
      <a href="/" style="color: black"
        ><img
          src="<%= '/images/eVote.png' %>"
          alt="logo"
          class="logo-img"
        /><span></span>
      </a>
    </div>
    <label for="fullname">Fullname</label>
    <input type="text" name="fullName" placeholder="John Example" />
    <label for="username">Username</label>
    <input type="text" name="username" placeholder="username" />
    <label for="email">Email</label>
    <input type="email" name="email" placeholder="example@gmail.com" />
    <label for="password">Password</label>
    <input type="password" name="password" placeholder="Enter your password" />
    <!-- <label for="password">Confirm Password</label>
    <input type="password" name="password" placeholder="Re-type password" /> -->
    <button type="submit">Sign Up</button>
    <h3 class="down-mobile">
      Already have an account? <a href="/Login">Login</a>
    </h3>
  </div>
  <div class="signupWe">
    <div class="signupWeh2">
      <h2>we are the <span>GOVERNMENT</span></h2>
    </div>
    <div class="signupimage">
      <img src="images/hand.png" alt="" width="300px" height="420px" />
    </div>
  </div>
</form>
